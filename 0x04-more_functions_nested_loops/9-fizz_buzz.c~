#include <stdio.h>

/**
 * main - void
 * description: print fizz and buzz and fizzbuzz
 * Return: 0
 */

int main(void)
{
	int i;
	for (i = 0; i <= 100; i++)
		if (i % 3 == 0)
			printf("Fizz");
		else if (i % 5 == 0)
			printf("buzz");
		else if ((i % 3 && i % 5) == 0)
			printf("FizzBuzz");
		else
			printf("%d", i);
	return (0);
}
